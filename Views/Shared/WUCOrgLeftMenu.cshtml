<script src="~/Scripts/bootstrap-leftmenu.js" type="text/javascript"></script>

<script type="text/javascript">
    $(document).ready(function () {
        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
            $(this).toggleClass('active');
        });
    });

    window.onload = function () {
        highlightSelectedMenu();
    }
    function highlightSelectedMenu() {
        var currentURL = window.location.href;
        if (currentURL.includes("Organization/Addmember")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseMembers").addClass("in");
            $("#liAddMember").removeClass("Organization_Color");
            $("#liAddMember").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/ViewMemberDetails")) {            
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseMembers").addClass("in");
            $("#liViewMember").removeClass("Organization_Color");
            $("#liViewMember").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/PendingPlanEnrollment")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseMembers").addClass("in");
            $("#liPendingEnrollmrntPlans").removeClass("Organization_Color");
            $("#liPendingEnrollmrntPlans").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/MembersReport")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseMembers").addClass("in");
            $("#liMembersReport").removeClass("Organization_Color");
            $("#liMembersReport").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/ViewPaymentDetails")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapsePayments").addClass("in");
            $("#liViewPayments").removeClass("Organization_Color");
            $("#liViewPayments").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/PaymentsReport")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapsePayments").addClass("in");
            $("#liPaymentsReport").removeClass("Organization_Color");
            $("#liPaymentsReport").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/PaymentTransactionReport")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapsePayments").addClass("in");
            $("#liTransactionReport").removeClass("Organization_Color");
            $("#liTransactionReport").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/ViewProviders")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapsePlans").addClass("in");
            $("#liViewProviders").removeClass("Organization_Color");
            $("#liViewProviders").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/AddDoctor")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapsePlans").addClass("in");
            $("#liViewProviders").removeClass("Organization_Color");
            $("#liViewProviders").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/SubscribePlans")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapsePlans").addClass("in");
            $("#liSubscribePlans").removeClass("Organization_Color");
            $("#liSubscribePlans").addClass("stripePanel");
        }


        if (currentURL.includes("Organization/OrganizationProfileDetails")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseAccount").addClass("in");
            $("#liOrganizationProfileDetails").removeClass("Organization_Color");
            $("#liOrganizationProfileDetails").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/ViewUsers")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseAccount").addClass("in");
            $("#liViewUsers").removeClass("Organization_Color");
            $("#liViewUsers").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/AddUser")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseAccount").addClass("in");
            $("#liViewUsers").removeClass("Organization_Color");
            $("#liViewUsers").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/ChangePassword")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseAccount").addClass("in");
            $("#liChangePassword").removeClass("Organization_Color");
            $("#liChangePassword").addClass("stripePanel");
        }

        if (currentURL.includes("Organization/BillingDashboard")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseBilling").addClass("in");
            $("#liBillingSubmission").removeClass("Organization_Color");
            $("#liBillingSubmission").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/ClaimSubmission")) {
            $('div[id^="collapse"]').removeClass("in");
            $("#collapseBilling").addClass("in");
            $("#liClaimSubmission").removeClass("Organization_Color");
            $("#liClaimSubmission").addClass("stripePanel");
        }
    }
</script>
