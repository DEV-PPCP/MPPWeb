                //            ProviderID: e.ProviderID,
                //            ProviderName: e.ProviderName,
                //            ProviderAddress: e.ProviderAddress
                //        }
                //    });
                //})(PlansList);
                $("#gridProviders").data("kendoGrid").dataSource.data(providerList);

                FilteredPlans = PlansList;

                //$("#PlansGrid").data("kendoGrid").dataSource.data(PlansList);
                //$("#PlansPayment").data("kendoGrid").dataSource.data(PlansList);

                //let group = PlansList.map((item) => item.OrganizationName).filter((item, i, ar) => ar.indexOf(item) === i).sort((a, b) => a - b).map(item => {
                //    let new_list = PlansList.filter(itm => itm.OrganizationName == item).map(itm => itm.OrganizationID);
                //    return { group: item, color: new_list }
                //});
                //console.log(group);
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                res = '';
